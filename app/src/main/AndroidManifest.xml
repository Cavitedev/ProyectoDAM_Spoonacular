<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.cavitedet.proyectodam_spoonacular">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
            android:name=".application.Aplicacion"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:fullBackupContent="true"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.ProyectoDAM_Spoonacular">
        <activity android:name="com.cavitedet.proyectodam_spoonacular.application.ActividadDeBusqueda">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".application.pantallas.listado.ActividadDeListado" />
        <activity android:name=".application.pantallas.detalles.ActividadDetalles" />
    </application>

</manifest>